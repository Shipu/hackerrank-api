{
    "name": "shipu/hackerrank-api",
    "description": "PHP and Laravel HackerRank Code Checker API",
    "keywords": ["Hackerrank", "code", "checker","online", "judge", "laravel"],
    "type": "library",
    "license": "CC(1.0)",
    "homepage": "http://shipuahamed.com",
    "authors": [
        {
            "name": "Shipu Ahamed",
            "email": "shipuahamed01@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "graham-campbell/testbench": "^3.3",
        "phpunit/phpunit": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Shipu\\HackerRank\\": "src/"
        },
        "files": [
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Shipu\\HackerRank\\HackerRankServiceProvider"
            ],
            "aliases": {
                "HackerRank": "Shipu\\HackerRank\\Facades\\HackerRank"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
